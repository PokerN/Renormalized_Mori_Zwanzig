Cell[CellGroupData[{

Cell[TextData[{
 "This procedure is iterative and we can write down a ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " Module for doing it "
}], "Subsubsection",
 CellChangeTimes->{{3.696875455059608*^9, 3.696875500459675*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"returnPert", "[", "order_", "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "problems", ",", "initConds", ",", "odeOp", ",", " ", "yeps", ",", "k", 
      ",", "soln", ",", "\[FormalEpsilon]"}], "}"}], ",", 
    "\[IndentingNewLine]", "\t\t\t\t", 
    RowBox[{
     RowBox[{"yeps", " ", "=", " ", 
      RowBox[{"Sum", "[", " ", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"y", "[", "k", "]"}], "[", "x", "]"}], " ", 
         RowBox[{"\[FormalEpsilon]", "^", "k"}]}], " ", ",", " ", 
        RowBox[{"{", 
         RowBox[{"k", ",", "1", ",", "order"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\t\t\t\t", 
     RowBox[{"odeOp", " ", "=", " ", 
      RowBox[{
       RowBox[{
        RowBox[{"D", "[", 
         RowBox[{"#", ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", "2"}], "}"}]}], "]"}], " ", "-", " ", 
        RowBox[{"#", "^", "2"}]}], " ", "&"}]}], ";", "\[IndentingNewLine]", 
     "\t\t\t\t", 
     RowBox[{"problems", " ", "=", " ", 
      RowBox[{"Delete", "[", 
       RowBox[{
        RowBox[{
        "CoefficientList", "[", " ", "\[IndentingNewLine]", 
         "\t\t\t\t\t\t\t\t\t\t", 
         RowBox[{
          RowBox[{
           RowBox[{"odeOp", " ", "@", " ", "yeps"}], "//", "Expand"}], ",", 
          " ", "\[FormalEpsilon]"}], "\[IndentingNewLine]", "\t\t\t\t\t\t\t", 
         "]"}], ",", "1"}], "]"}]}], ";", "\[IndentingNewLine]", "\t\t\t\t", 
     RowBox[{"initConds", " ", "=", 
      RowBox[{
      "Table", "[", " ", "\[IndentingNewLine]", "\t\t\t\t\t\t\t\t\t", 
       RowBox[{
        RowBox[{
         RowBox[{"Piecewise", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"{", " ", 
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"y", "[", "k", "]"}], "[", "0", "]"}], " ", 
               "\[Equal]", " ", "1"}], ",", " ", 
              RowBox[{"k", "\[Equal]", "1"}]}], "}"}], "}"}], ",", 
           RowBox[{
            RowBox[{
             RowBox[{"y", "[", "k", "]"}], "[", "0", "]"}], " ", "\[Equal]", 
            " ", "0"}]}], "]"}], " ", "\[IndentingNewLine]", 
         "\t\t\t\t\t\t\t\t      ", "&&", " ", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"y", "[", "k", "]"}], "'"}], "[", "0", "]"}], "\[Equal]", 
          " ", "0"}]}], ",", "\[IndentingNewLine]", "\t\t\t\t\t\t   ", 
        RowBox[{"{", 
         RowBox[{"k", ",", "1", ",", "order"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\t\t\t\t", 
     RowBox[{"For", "[", " ", 
      RowBox[{
       RowBox[{"k", " ", "=", " ", "1"}], ",", " ", 
       RowBox[{"k", " ", "\[LessEqual]", " ", "order"}], ",", " ", 
       RowBox[{"k", "++"}], ",", "\[IndentingNewLine]", "\t\t\t\t\t", 
       RowBox[{
        RowBox[{"soln", " ", "=", 
         RowBox[{
          RowBox[{"DSolve", "[", " ", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"problems", "[", 
               RowBox[{"[", "k", "]"}], "]"}], " ", "\[Equal]", "0"}], " ", "&&",
              " ", 
             RowBox[{"initConds", "[", 
              RowBox[{"[", "k", "]"}], "]"}]}], ",", " ", 
            RowBox[{
             RowBox[{"y", "[", "k", "]"}], "[", "x", "]"}], ",", "x"}], "]"}],
           "[", 
          RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        "\t\t\t\t\t", 
        RowBox[{"problems", " ", "=", " ", 
         RowBox[{"problems", " ", "/.", " ", "soln"}]}], ";", 
        "\[IndentingNewLine]", "\t\t\t\t\t", 
        RowBox[{"yeps", " ", "=", " ", 
         RowBox[{"yeps", " ", "/.", " ", "soln"}]}]}]}], 
      "\[IndentingNewLine]", "\t\t\t\t", "]"}], ";", "\[IndentingNewLine]", 
     "\t\t\t\t", 
     RowBox[{"yeps", " ", "/.", " ", 
      RowBox[{"{", " ", 
       RowBox[{"\[FormalEpsilon]", " ", "\[Rule]", " ", "\[Epsilon]"}], 
       "}"}]}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.6968738065837717`*^9, 3.696873853882946*^9}, {
  3.6968739173761463`*^9, 3.6968743075717487`*^9}, {3.6968744520217457`*^9, 
  3.6968744714091682`*^9}, {3.696874854732506*^9, 3.6968748658563833`*^9}, {
  3.696875167684676*^9, 3.696875170764151*^9}, {3.6968757276493807`*^9, 
  3.696875770537573*^9}}],

